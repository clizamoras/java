    double calculateDA(double bp) { return bp * 0.60; }
}

static class A2_Employee_Payroll extends JFrame {

    JTextField tName, tId, tBp;
    JRadioButton rProf, rAssoc, rAsst;

    A2_Employee_Payroll() {

        setTitle("A2 Employee Payroll");
        setSize(700, 300);
        setLayout(new GridLayout(0, 2, 5, 5));

        tName = new JTextField();
        tId = new JTextField();
        tBp = new JTextField();

        rProf = new JRadioButton("Professor", true);
        rAssoc = new JRadioButton("Associate Professor");
        rAsst = new JRadioButton("Assistant Professor");

        ButtonGroup bg = new ButtonGroup();
        bg.add(rProf); bg.add(rAssoc); bg.add(rAsst);

        JButton btn = new JButton("Generate Pay Slip");

        add(new JLabel("Employee Name")); add(tName);
        add(new JLabel("Employee ID")); add(tId);
        add(new JLabel("Basic Pay")); add(tBp);
        add(new JLabel("Role"));
        add(new JPanel() {{ add(rProf); add(rAssoc); add(rAsst); }});
        add(btn);

        btn.addActionListener(e -> calculate());

        setVisible(true);
        setDefaultCloseOperation(JFrame.DISPOSE_ON_CLOSE);
    }

    private void calculate() {
        try {
            double bp = Double.parseDouble(tBp.getText());
            Employee emp;

            if (rProf.isSelected())
                emp = new Professor(tName.getText(), tId.getText(), bp);
            else if (rAssoc.isSelected())
                emp = new AssociateProfessor(tName.getText(), tId.getText(), bp);
            else
                emp = new AssistantProfessor(tName.getText(), tId.getText(), bp);

            JOptionPane.showMessageDialog(this,
                    emp.generatePaySlip(),
                    "Pay Slip",
                    JOptionPane.INFORMATION_MESSAGE);

        } catch (Exception ex) {
            JOptionPane.showMessageDialog(this,"Invalid Input","Error", JOptionPane.ERROR_MESSAGE);
        }
    }
}



// Main class for Shape Demo with radio buttons
public class A3_Shape_Demo_Radio extends JFrame {

    // Input fields
    JTextField tL, tW, tB, tH, tR;
    JTextArea output;

    // Radio buttons for shape selection
    JRadioButton rectBtn, triBtn, circBtn;
    ButtonGroup group;

    A3_Shape_Demo_Radio() {
        setTitle("A3: Shape Area Demo");
        setSize(400, 400);
        setLayout(new BorderLayout());

        // Panel for radio buttons
        JPanel radioPanel = new JPanel(new FlowLayout());
        rectBtn = new JRadioButton("Rectangle");
        triBtn = new JRadioButton("Triangle");
        circBtn = new JRadioButton("Circle");

        group = new ButtonGroup();
        group.add(rectBtn);
        group.add(triBtn);
        group.add(circBtn);

        radioPanel.add(rectBtn);
        radioPanel.add(triBtn);
        radioPanel.add(circBtn);

        add(radioPanel, BorderLayout.NORTH);

        // Panel for inputs
        JPanel input = new JPanel(new GridLayout(5, 2, 5, 5));
        tL = new JTextField();
        tW = new JTextField();
        tB = new JTextField();
        tH = new JTextField();
        tR = new JTextField();

        input.add(new JLabel("Rectangle Length"));
        input.add(tL);
        input.add(new JLabel("Rectangle Width"));
        input.add(tW);
        input.add(new JLabel("Triangle Base"));
        input.add(tB);
        input.add(new JLabel("Triangle Height"));
        input.add(tH);
        input.add(new JLabel("Circle Radius"));
        input.add(tR);

        add(input, BorderLayout.CENTER);

        // Button to calculate area
        JButton btn = new JButton("Calculate Area");
        add(btn, BorderLayout.SOUTH);

        output = new JTextArea(5, 20);
        output.setEditable(false);
        add(new JScrollPane(output), BorderLayout.EAST);

        // Action listener for calculate button
        btn.addActionListener(e -> calculate());

        setVisible(true);
        setDefaultCloseOperation(JFrame.DISPOSE_ON_CLOSE);
    }

    void calculate() {
        try {
            if (rectBtn.isSelected()) {
                double length = Double.parseDouble(tL.getText());
                double width = Double.parseDouble(tW.getText());
                output.setText("Rectangle Area: " + (length * width));
            } else if (triBtn.isSelected()) {
                double base = Double.parseDouble(tB.getText());
                double height = Double.parseDouble(tH.getText());
                output.setText("Triangle Area: " + (0.5 * base * height));
            } else if (circBtn.isSelected()) {
                double radius = Double.parseDouble(tR.getText());
                output.setText("Circle Area: " + (3.14159 * radius * radius));
            } else {
                output.setText("Please select a shape");
            }
        } catch (Exception ex) {
            output.setText("Invalid Input");
        }
    }

    public static void main(String[] args) {
        new A3_Shape_Demo_Radio();
    }
}


    
   
   
   

public class A4_Finally_Demo extends JFrame {

    JTextArea text;
    JRadioButton noExceptionBtn, withExceptionBtn, withReturnBtn;
    ButtonGroup group;

   A4_Finally_Demo() {
        setTitle("Finally Block Demo");
        setSize(500, 400);
        setLayout(new BorderLayout());

        // Radio buttons for the three cases
        noExceptionBtn = new JRadioButton("No Exception");
        withExceptionBtn = new JRadioButton("With Exception");
        withReturnBtn = new JRadioButton("With Return");

        group = new ButtonGroup();
        group.add(noExceptionBtn);
        group.add(withExceptionBtn);
        group.add(withReturnBtn);

        JPanel top = new JPanel();
        top.add(noExceptionBtn);
        top.add(withExceptionBtn);
        top.add(withReturnBtn);

        add(top, BorderLayout.NORTH);

        text = new JTextArea();
        text.setEditable(false);
        add(new JScrollPane(text), BorderLayout.CENTER);

        JButton runBtn = new JButton("Run Case");
        add(runBtn, BorderLayout.SOUTH);

        // Action when "Run Case" button is pressed
        runBtn.addActionListener(e -> runSelectedCase());

        setVisible(true);
        setDefaultCloseOperation(JFrame.DISPOSE_ON_CLOSE);
    }

    void runSelectedCase() {
        if (noExceptionBtn.isSelected()) {
            text.setText(noException());
        } else if (withExceptionBtn.isSelected()) {
            text.setText(withException());
        } else if (withReturnBtn.isSelected()) {
            text.setText(withReturn());
        } else {
            text.setText("Please select a case.");
        }
    }

    

    String noException() {
        StringBuilder output = new StringBuilder();
        try {
            output.append("try of no exception\n");
        } catch (Exception e) {
            output.append("catch of no exception\n");
        } finally {
            output.append("finally of no exception\n");
        }
        output.append("end of no exception\n");
        return output.toString();
    }

    String withException() {
        StringBuilder output = new StringBuilder();
        try {
            output.append("try of with exception\n");
            int a = 5 / 0; // This will throw ArithmeticException
        } catch (Exception e) {
            output.append("catch of with exception\n");
        } finally {
            output.append("finally of with exception\n");
        }
        output.append("end of with exception\n");
        return output.toString();
    }

    String withReturn() {
        StringBuilder output = new StringBuilder();
        try {
            output.append("try of with return\n");
            return output.toString(); // Returning from try
        } catch (Exception e) {
            output.append("catch of with return\n");
        } finally {
            output.append("finally of with return\n"); // finally still executes
        }
        output.append("end of with return\n");
        return output.toString();
    }

  
    public static void main(String[] args) {
        new A4_Finally_Demo();
    }
}

  

public class UserExceptionJobDemo extends JFrame {

    JTextField nameField, roleField, ageField;
    JTextArea result;

    UserExceptionJobDemo() {
        setTitle("Job Application Eligibility");
        setSize(700, 500);
        setLayout(new GridLayout(5, 2, 5, 5));

        // Input fields
        add(new JLabel("Name:"));
        nameField = new JTextField();
        add(nameField);

        add(new JLabel("Role Applied For:"));
        roleField = new JTextField();
        add(roleField);

        add(new JLabel("Age:"));
        ageField = new JTextField();
        add(ageField);

        JButton checkBtn = new JButton("Check Eligibility");
        add(checkBtn);

        result = new JTextArea();
        result.setEditable(false);
        add(result);

        // Button action
        checkBtn.addActionListener(e -> checkEligibility());

        setVisible(true);
        setDefaultCloseOperation(JFrame.DISPOSE_ON_CLOSE);
    }

    void checkEligibility() {
        try {
            String name = nameField.getText().trim();
            String role = roleField.getText().trim();
            int age = Integer.parseInt(ageField.getText().trim());

            if (age < 18) {
                throw new TooEarlyToApplyException();
            } else if (age > 45) {
                throw new TooLateToApplyException();
            } else {
                result.setText("Application Successful!\n"
                        + "Name: " + name + "\n"
                        + "Role: " + role + "\n"
                        + "Age: " + age);
            }
        } catc

