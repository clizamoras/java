import java.util.*;
class tooearlytoapply extends Exception{
    tooearlytoapply(){
        super("too early to apply");
    }
}
class toolatetoapply extends Exception{
    toolatetoapply(){
        super("too late to apply");
    }
}
public class main {
    public static void main(String args[]){
        Scanner sc=new Scanner(System.in);
        try{
            System.out.println("name");
            String name=sc.nextLine();
            System.out.println("age");
            int age=sc.nextInt();
            if(age<18){
                throw new tooearlytoapply();
            }
            else if(age>40){
                throw new toolatetoapply();
            }
            else{
                System.out.println("name"+name);
                System.out.println("age"+age);
            }
        }
        catch(tooearlytoapply | toolatetoapply e){
            System.out.println("error"+ e.getMessage());
        }
        catch(Exception e){
            System.out.println("error");
        }
        finally{
            sc.close();
        }
    }
}